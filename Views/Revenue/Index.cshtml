@model IEnumerable<WebFastFood.Models.Revenue>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Doanh thu</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RevenueDateStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RevenueDateEnd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RevenueMoney)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RevenueDateStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RevenueDateEnd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RevenueMoney)
        </td>
    </tr>
}

</table>
